def double print(double x) {}
def double sqrt(double x) {}

def double pi(double n) {
	double a:=1.0
	double b:=(1/sqrt(2.0))
	double t:=(1/4.0)
	double p:=1.0

	for (double i:=0.0), i<n, i=(i+1) {
		double a2:=((a+b)/2)
		b=(sqrt(a*b))
		t=(t-(p*(a-a2)*(a-a2)))
		p=(2*p)
		a=a2
	}

	((a+b)*(a+b)/(4*t))	
}

def double main() {
	print(pi(3.0))
}
