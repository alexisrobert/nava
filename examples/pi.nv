def double print(double x) {}
def double sin(double x) {}

def double f(double x) {
	sin(x)
}

def double dicho(double a,double b,int n) {
	double c:=((a+b)/2)
	if n>0 {
		if (f(a)*f(c))>0.0 {
			dicho(c,b,n-1)
		} else {
			dicho(a,c,n-1)
		}
	} else {
		c
	}
}

def double main() {
	print(dicho(3.0,4.0,200))
}
